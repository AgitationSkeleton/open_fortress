"VertexLitGeneric"
{
	"$baseTexture" 	"models/weapons/w_tfc_medpack/tfc_medpack_custom"
	"$phong" "1"
	"$phongexponent" "5"

	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"

	"$bumpmap" "models/weapons/w_tfc_medpack/tfc_medpack_normal"
	
	"$blendtintbybasealpha"		"1"
	//"$blendtintcoloroverbase"		"1"
	"$colortint_base"		"{0 0 0}"
	"$color2"		"{93 72 117}"
	"$colortint_tmp"		"[0 0 0]"	

	"$tempvar" "0"

	"$basephongexponent" "15"
	"$basephongboost" "5"

	"$commadd_phongexponent" "40"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".25"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"


		


	"Proxies"
	{
		
		"ItemTintColor"
		{
			"resultVar"		"$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"		"$colortint_tmp"
			"srcVar2"		"$colortint_base"
			"resultVar"		"$color2"
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
